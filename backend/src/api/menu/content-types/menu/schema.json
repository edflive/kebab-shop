{
  "kind": "collectionType",
  "collectionName": "menus",
  "info": {
    "singularName": "menu",
    "pluralName": "menus",
    "displayName": "Menu"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "nom": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "prix": {
      "type": "decimal",
      "required": true
    },
    "produits": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::produit.produit"
    },
    "image": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": false
    }
  }
}